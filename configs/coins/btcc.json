{
  "coin": {
      "name": "Bcore",
      "shortcut": "BTCC",
      "label": "Bitcoin Core",
      "alias": "btcc"
  },
  "ports": {
    "backend_rpc": 8042,
    "backend_message_queue": 38342,
    "blockbook_internal": 9042,
    "blockbook_public": 9142
  },
  "ipc": {
    "rpc_url_template": "http://127.0.0.1:{{.Ports.BackendRPC}}",
    "rpc_user": "rpc",
    "rpc_pass": "rpc",
    "rpc_timeout": 25,
    "message_queue_binding_template": "tcp://127.0.0.1:{{.Ports.BackendMessageQueue}}"
  },
  "backend": {
    "package_name": "backend-bitcoincore",
    "package_revision": "satoshilabs-1",
    "system_user": "bitcoincore",
    "version": "0.16.2",
    "binary_url": "https://github.com/bitcoin-cored/bitcoin-cored/releases/download/v0.16.2/bitcoin-cored-0.16.2-x86_64-linux-gnu.tar.gz",
    "verification_type": "sha256",
    "verification_source": "493de3db8b7f94c6423039d231fb64a55fb0c29a9e6ce76e78f1b03cfd6468fa",
    "extract_command": "tar -C backend --strip 1 -xf",
    "exclude_files": [
        "bin/clashicd-qt"
    ],
    "exec_command_template": "{{.Env.BackendInstallPath}}/{{.Coin.Alias}}/bin/clashicd -datadir={{.Env.BackendDataPath}}/{{.Coin.Alias}}/backend -conf={{.Env.BackendInstallPath}}/{{.Coin.Alias}}/{{.Coin.Alias}}.conf -pid=/run/{{.Coin.Alias}}/{{.Coin.Alias}}.pid",
    "logrotate_files_template": "{{.Env.BackendDataPath}}/{{.Coin.Alias}}/backend/*.log",
    "postinst_script_template": "",
    "service_type": "forking",
    "service_additional_params_template": "",
    "protect_memory": true,
    "mainnet": true,
    "config_file": "clashicd.conf"
  },
  "blockbook": {
    "package_name": "blockbook-bitcoincore",
    "system_user": "blockbook-bitcoincore",
    "internal_binding_template": ":{{.Ports.BlockbookInternal}}",
    "public_binding_template": ":{{.Ports.BlockbookPublic}}",
    "explorer_url": "https://btcc-explorer.truevisionofsatoshi.com",
    "additional_params": "",
    "block_chain": {
      "parse": true,
      "subversion": "/Bitcoin Core SQ:0.16.2/",
      "address_format": "legacy",
      "mempool_workers": 8,
      "mempool_sub_workers": 2,
      "block_addresses_to_keep": 300,
      "additional_params": {}
    }
  },
  "meta": {
    "package_maintainer": "Cryptagoras",
    "package_maintainer_email": "cryptagoras@protonmail.ch"
  }
}
